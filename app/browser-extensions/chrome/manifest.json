{
  "name": "Onote",
  "description": "Fliex note from the browser during your browsing session.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Onote",
    "default_icon": "icon.png"
  },
  "permissions": ["nativeMessaging"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["*://*/*"]
}
